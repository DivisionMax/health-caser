<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.csc3003.healthcaser" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".LoadHealthCaseActivity"
            android:label="@string/title_activity_load_health_case" >
        </activity>
        <!-- landing page -->
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ChooseCaseActivity"
            android:label="@string/title_activity_choose_case" >
        </activity>
        <activity
            android:name=".LActivity"
            android:label="@string/title_activity_l" >
        </activity>

    </application>

</manifest>
